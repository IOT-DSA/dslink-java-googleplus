{
  "name": "dslink-java-googleplus",
  "version": "0.1.1",
  "description": "A dslink for google+",
  "license": "Apache",
  "main": "bin/dslink-java-googleplus",
  "author": {
    "name": "Daniel Shapiro",
    "email": "danielsspam1272@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-java-googleplus"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-java-googleplus/issues"
  },
  "configs": {
  	"name": {
      "type": "string",
      "default": "GooglePlus"
    },
    "broker": {
      "type": "url"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
     "handler_class": {
      "type": "string",
      "default": "googleplus.Main"
    }
  }
}
